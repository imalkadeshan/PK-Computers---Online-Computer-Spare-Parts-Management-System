     </servlet>
     
     <servlet-mapping>
     	<servlet-name>acceptOrderServlet</servlet-name>
     	<url-pattern>/acceptOrderServlet</url-pattern>
     </servlet-mapping>
     
      <servlet>
     	<servlet-name>declineOrderServlet</servlet-name>
     	<servlet-class>pk.servlet.declineOrderServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>declineOrderServlet</servlet-name>
     	<url-pattern>/declineOrderServlet</url-pattern>
     </servlet-mapping>
     


</web-app>